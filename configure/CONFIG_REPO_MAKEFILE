

REPO_MAKEFILE_INPUT_OPTIONS := -C $(REPO_SRC_PATH)
REPO_MAKEFILE_INPUT_OPTIONS += -f $(REPO_MAKEFILE)
REPO_MAKEFILE_INPUT_OPTIONS +=  DESTDIR="$(DESTDIR)"
REPO_MAKEFILE_INPUT_OPTIONS +=  REPO_BASE="$(REPO_BASE)"
REPO_MAKEFILE_INPUT_OPTIONS +=  REPO_DOWNLOADER_BASE="$(REPO_DOWNLOADER_BASE)"
REPO_MAKEFILE_INPUT_OPTIONS +=  REPO_SNMP_MIBS_DIRS="$(REPO_SNMP_MIBS_DIRS)"


REPO_MAKE_CMDS:=make $(REPO_MAKEFILE_INPUT_OPTIONS)

VARS_EXCLUDES+=REPO_MAKEFILE_INPUT_OPTIONS
